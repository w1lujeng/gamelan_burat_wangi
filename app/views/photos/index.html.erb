<div class="row">
  <div class="col-md-4">
 
  <div>
  <div>
    <h2>Burat Wangi Photo Page</h2>
      <ul>
        <% if @photos.count == 0 %>
          <h5>No Photos Yet!</h5>
        <% else %>
          <% @photos.each do |photo| %>
              
              <%= image_tag photo.image.url, class: "medium" %>
              <%= photo.content %>
              <div>
              <br>
              <%= link_to "edit photo", edit_photo_path(photo), class: "btn   btn-xs btn-warning text-right" if photo.user == current_user %>         
              <%= link_to "delete photo", photo_path(photo), class: "btn btn-xs btn-danger", method: :delete, data: { confirm:
              "Are you sure you want to delete this post?"} if photo.user == current_user %>       
            </div>         
              <br>
              <br>
          <% end %>
        <% end %>
      </ul>
  </div>
  <div class="col-md-4">
  </div>
 </div>
